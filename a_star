# Importing Maze from Pyamaze
from pyamaze import maze

row, col = 5, 5


# Function to create a maze
def createMaze(row, col):
    m = maze(row, col)
    m.CreateMaze()
    m.run()


# Creating the Maze
createMaze(row, col)


